<h1>Create a new breed:</h1>

<%= form_tag "/dog", method: :post do %>
  <div>
    <%= label_tag :breed %>
    <%= text_field_tag :input_breed %>
  </div>
  <div>
    <%= label_tag :group %>
    <%= text_field_tag :input_group %>
  </div>
  <div>
    <%= submit_tag 'Create a new breed' %>
  </div>


<% end %>